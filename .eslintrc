{
  "extends": "standard",
  "plugins": [
    "react"
  ],
  "rules": {
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "jsx-quotes": [
      "error",
      "prefer-double"
    ]
  }
}
